{
  "name": "otus-css-layout-turarov",
  "version": "1.0.0",
  "description": "Three column layout",
  "main": "index.js",
  "scripts": {
    "build-webpack": "webpack -c webpack.config.js",
    "build-prod": "webpack --mode production -c webpack.config.prod.js",
    "build": "webpack --mode=development",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build-dev": "webpack --watch",
    "start-dev": "webpack serve",
    "start": "webpack serve --mode development -c webpack.config.js",
    "test": "jest",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "otus-css-layout-turarov"
  },
  "author": "Turarov K.",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "at-rule-packer": "^0.4.1",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-css": "^0.8.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.30",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
